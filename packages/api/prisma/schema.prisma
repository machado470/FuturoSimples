generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  createdAt DateTime @default(now())
}

model Scenario {
  id        String   @id @default(cuid())
  userId    String
  payload   Json
  result    Json
  createdAt DateTime @default(now())

  @@index([userId])
}
